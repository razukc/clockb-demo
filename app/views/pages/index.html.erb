

<!-- Sub menu -->
<div class="row">
	<div class="large-12 columns">
	<div data-magellan-expedition="fixed">
		<dl class="sub-nav right">
	    	<dd data-magellan-arrival="events"><a href="#events">EVENTS</a></dd>
	    	<dd data-magellan-arrival="whyclockb"><a href="#whyclockb">WHY CLOCK B?</a></dd>
	    	<dd data-magellan-arrival="services"><a href="#services">SERVICES</a></dd>
	    	<dd data-magellan-arrival="packages_decoy"><a href="#packages">PACKAGES</a></dd>
	  	</dl>
	</div>
	</div>
</div>
<br />
<!-- Slider -->
<% if @sliders.any? %>
<div class="row">
	<ul class="example-orbit" data-orbit>
		<% @sliders.each do |slider| %>
	  <li> <!-- A92B48/fff -->
	  	<%= image_tag slider.slide, class: "lazyload" %>
	    <!-- <img src="1000x500.png" alt="slide 1" class="lazyload"/> -->
	    <% if !slider.caption.empty? %>
	    <div class="orbit-caption">
	      <%= slider.caption %>
	    </div>
	    <% end %>
	  </li>
	  <% end %>
	</ul><hr />
</div>
<% end %>
<div class="row"><a name="events"></a></div>
<div class="row">
	<!-- Event Calendar -->
	
  <div class="medium-6 large-4 columns">
  	<div id="events_calendar">
  		<%= render partial: 'layouts/calendar' %>
	</div>
	<%= link_to "View all Events", events_path, class: 'button expand' %>
  </div>
  <!-- Event Listing -->
  <div class="medium-6 large-8 columns">
  	
  	<h3 data-magellan-destination="events">Upcoming Events</h3>
  	<%= render "layouts/upcoming_events" %>
  	<br />
  </div><hr />
</div>

